    <style>
        /* Add background color to the navbar */
        .navbar {
            background-color: #FBFAF9;
            border-top: 1px
        }
       
        /* Space out the navbar items and remove the bullet points */
        ul.navbar-nav {
            list-style: none;
            display: flex;
            gap: 20px; /* Adjust the spacing between navbar items */
            justify-content: flex-end; /* Align items to the right */
            font-size: 20px;
        }




        ul.navbar-nav {
        list-style: none; /* Remove the bullet */
        display: inline; /* Display the list items inline */
        float:right;
        padding :1px;
        font-size:16px
        }
        ul.navbar-nav li {
        display: inline-block; /* Make the list items display inline */
        margin-left: 3px;
        margin-right: 3px;
        }
        a:link {
        text-decoration: none;
        }
        a:visited {
        text-decoration: none;
        }
        a:hover {
        text-decoration: none;
        }
        a:active {
        text-decoration: none;
        }
        html,
        body {
        height: 100%;
        
        }
        body {
        display: flex;
        flex-direction: column;
        
        }
        main {
        flex: 1;
        
        }
        main.”container”{
            padding:20px;
        }
        footer {
        background-color: #29AF6F;
        color: #ffffff;
        
        
        }
        footer .navbar-text {
        margin-bottom: 0;
        font-size: x-small;
        padding: 5px;
        
        }

